    "react": "^16.13.1",
    "react-div-100vh": "^0.3.8",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "react-text-mask": "^5.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@types/jest": "^24.9.1",
